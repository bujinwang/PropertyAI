{
  "name": "propertymanagementai-backend",
  "version": "1.0.0",
  "description": "Backend for PropertyManagementAI application",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:python": "python src/predictive-analytics/src/deployment/deploy_model.py",
    "dev": "concurrently \"npm:dev:node\" \"npm:start:python\"",
    "dev:node": "node --require ts-node/register src/index.ts",
    "build": "tsc",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "lint": "eslint . --ext .ts",
    "test": "jest --watchman=false --runInBand",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "migrate:rental": "npx prisma generate && npx ts-node scripts/migrate-to-rental.ts",
    "migrate:permissions": "npx ts-node scripts/update-rental-permissions.ts",
    "check:migration": "npx ts-node check-migration-status.ts",
    "cleanup:legacy": "ts-node scripts/cleanup-legacy-migration.ts",
    "cleanup:legacy:dry-run": "ts-node scripts/cleanup-legacy-migration.ts --dry-run",
    "test:migration": "jest tests/migration-helpers.test.ts",
    "test:no-legacy": "jest --testPathIgnorePatterns=/legacy-tests/",
    "update:integrations": "ts-node scripts/update-external-integrations.ts",
    "monitor:deprecated": "curl http://localhost:3001/api/monitoring/deprecated-usage",
    "schedule:cleanup": "ts-node scripts/schedule-cleanup.ts",
    "final:cleanup": "ts-node scripts/cleanup-legacy-migration.ts --final"
  },
  "keywords": [
    "property",
    "management",
    "ai",
    "backend",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
