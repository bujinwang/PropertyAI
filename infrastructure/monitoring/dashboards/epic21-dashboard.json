{
  "dashboard": {
    "title": "Epic 21 AI Features Production Monitoring",
    "description": "Comprehensive monitoring dashboard for Epic 21 AI-powered features in production",
    "tags": ["epic21", "ai-features", "production-monitoring"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"propertyai-production\"}",
            "legendFormat": "System Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        }
      },
      {
        "id": 2,
        "title": "API Response Times",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"propertyai-production\", route=~\"/api/.*\"}[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"propertyai-production\", route=~\"/api/.*\"}[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "seconds"
          }
        }
      },
      {
        "id": 3,
        "title": "Predictive Maintenance Performance",
        "type": "table",
        "targets": [
          {
            "expr": "rate(epic21_maintenance_predictions_total[5m])",
            "legendFormat": "Predictions/min"
          },
          {
            "expr": "rate(epic21_maintenance_alerts_total[5m])",
            "legendFormat": "Alerts/min"
          },
          {
            "expr": "epic21_maintenance_accuracy_ratio",
            "legendFormat": "Prediction Accuracy"
          }
        ]
      },
      {
        "id": 4,
        "title": "Tenant Churn Analysis",
        "type": "bargauge",
        "targets": [
          {
            "expr": "epic21_churn_predictions_total{risk_level=\"high\"}",
            "legendFormat": "High Risk Tenants"
          },
          {
            "expr": "epic21_churn_predictions_total{risk_level=\"medium\"}",
            "legendFormat": "Medium Risk Tenants"
          },
          {
            "expr": "epic21_churn_predictions_total{risk_level=\"low\"}",
            "legendFormat": "Low Risk Tenants"
          }
        ]
      },
      {
        "id": 5,
        "title": "Market Intelligence Data Freshness",
        "type": "stat",
        "targets": [
          {
            "expr": "time() - epic21_market_data_last_update",
            "legendFormat": "Data Age (seconds)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "seconds",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 3600
                },
                {
                  "color": "red",
                  "value": 7200
                }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "AI Model Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(epic21_ai_model_inference_duration_seconds_sum[5m]) / rate(epic21_ai_model_inference_duration_seconds_count[5m])",
            "legendFormat": "Avg Inference Time"
          },
          {
            "expr": "epic21_ai_model_error_rate",
            "legendFormat": "Model Error Rate"
          }
        ]
      },
      {
        "id": 7,
        "title": "Feature Usage Analytics",
        "type": "table",
        "targets": [
          {
            "expr": "increase(epic21_feature_usage_total{feature=\"predictive_maintenance\"}[1h])",
            "legendFormat": "Predictive Maintenance"
          },
          {
            "expr": "increase(epic21_feature_usage_total{feature=\"churn_prediction\"}[1h])",
            "legendFormat": "Churn Prediction"
          },
          {
            "expr": "increase(epic21_feature_usage_total{feature=\"market_intelligence\"}[1h])",
            "legendFormat": "Market Intelligence"
          },
          {
            "expr": "increase(epic21_feature_usage_total{feature=\"ai_reporting\"}[1h])",
            "legendFormat": "AI Reporting"
          },
          {
            "expr": "increase(epic21_feature_usage_total{feature=\"risk_assessment\"}[1h])",
            "legendFormat": "Risk Assessment"
          }
        ]
      },
      {
        "id": 8,
        "title": "Error Rate by Feature",
        "type": "bargauge",
        "targets": [
          {
            "expr": "rate(epic21_feature_errors_total{feature=\"predictive_maintenance\"}[5m]) / rate(epic21_feature_requests_total{feature=\"predictive_maintenance\"}[5m])",
            "legendFormat": "Predictive Maintenance"
          },
          {
            "expr": "rate(epic21_feature_errors_total{feature=\"churn_prediction\"}[5m]) / rate(epic21_feature_requests_total{feature=\"churn_prediction\"}[5m])",
            "legendFormat": "Churn Prediction"
          },
          {
            "expr": "rate(epic21_feature_errors_total{feature=\"market_intelligence\"}[5m]) / rate(epic21_feature_requests_total{feature=\"market_intelligence\"}[5m])",
            "legendFormat": "Market Intelligence"
          },
          {
            "expr": "rate(epic21_feature_errors_total{feature=\"ai_reporting\"}[5m]) / rate(epic21_feature_requests_total{feature=\"ai_reporting\"}[5m])",
            "legendFormat": "AI Reporting"
          },
          {
            "expr": "rate(epic21_feature_errors_total{feature=\"risk_assessment\"}[5m]) / rate(epic21_feature_requests_total{feature=\"risk_assessment\"}[5m])",
            "legendFormat": "Risk Assessment"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 0.05
                },
                {
                  "color": "red",
                  "value": 0.1
                }
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Database Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mysql_global_status_queries[5m])",
            "legendFormat": "Queries/sec"
          },
          {
            "expr": "mysql_global_status_threads_connected",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "rate(mysql_global_status_slow_queries[5m])",
            "legendFormat": "Slow Queries/sec"
          }
        ]
      },
      {
        "id": 10,
        "title": "Resource Utilization",
        "type": "graph",
        "targets": [
          {
            "expr": "100 - (avg by(instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "Memory Usage %"
          },
          {
            "expr": "(node_filesystem_avail_bytes{mountpoint=\"/\"} / node_filesystem_size_bytes{mountpoint=\"/\"}) * 100",
            "legendFormat": "Disk Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        }
      },
      {
        "id": 11,
        "title": "User Experience Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"propertyai-frontend\", route=\"/\"}[5m]))",
            "legendFormat": "Page Load (95th percentile)"
          },
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "seconds"
          }
        }
      },
      {
        "id": 12,
        "title": "Alert Summary",
        "type": "table",
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\"}",
            "legendFormat": "{{alertname}}"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "timepicker": {
      "refresh_intervals": [
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    }
  }
}