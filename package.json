  "name": "propertyai",
  "version": "1.0.0",
  "description": "AI-powered property management platform with intelligent automation, predictive analytics, and comprehensive rental management features",
  "keywords": [
    "property-management",
    "artificial-intelligence",
    "real-estate",
    "rental-management",
    "predictive-analytics",
    "maintenance-management",
    "tenant-portal",
    "ai-automation",
    "property-tech",
    "proptech"
  ],
  "homepage": "https://github.com/bujinwang/PropertyAI#readme",
  "bugs": {
    "url": "https://github.com/bujinwang/PropertyAI/issues"
