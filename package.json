  "contributors": [
    {
      "name": "PropertyAI Community",
      "url": "https://github.com/PropertyAI/PropertyAI/graphs/contributors"
    }
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "private": false,
  "workspaces": [
    "backend",
    "dashboard",
    "ContractorApp",
    "propertyapp"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:dashboard\"",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:dashboard": "npm run dev --workspace=dashboard",
    "dev:contractor": "npm run start --workspace=ContractorApp",
    "dev:propertyapp": "npm run start --workspace=propertyapp",
    "build": "npm run build --workspaces",
    "build:backend": "npm run build --workspace=backend",
    "build:dashboard": "npm run build --workspace=dashboard",
    "build:contractor": "npm run build --workspace=ContractorApp",
    "build:propertyapp": "npm run build --workspace=propertyapp",
    "test": "npm run test --workspaces",
    "test:backend": "npm run test --workspace=backend",
    "test:dashboard": "npm run test --workspace=dashboard",
    "test:contractor": "npm run test --workspace=ContractorApp",
    "test:propertyapp": "npm run test --workspace=propertyapp",
    "lint": "npm run lint --workspaces",
    "lint:backend": "npm run lint --workspace=backend",
    "lint:dashboard": "npm run lint --workspace=dashboard",
    "lint:contractor": "npm run lint --workspace=ContractorApp",
    "lint:propertyapp": "npm run lint --workspace=propertyapp",
    "clean": "npm run clean --workspaces",
    "install:all": "npm install"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/long": "^4.0.2",
    "@vitejs/plugin-react": "^4.5.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@types/node-schedule": "^2.1.7",
    "@types/pdfkit": "^0.13.9",
    "axios": "^1.11.0",
    "dotenv-cli": "^9.0.0",
    "firebase-admin": "^13.4.0",
    "googleapis": "^150.0.1",
    "node-schedule": "^2.1.1",
    "pdfkit": "^0.17.1",
    "plaid": "^37.0.0"
  },
  "overrides": {
    "ip": "^2.0.1",
